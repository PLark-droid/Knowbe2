{
  "name": "knowbe2",
  "version": "0.20.0",
  "description": "Knowbe2 - Miyabi Framework Autonomous Development Platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "agents:parallel:exec": "tsx src/agents/coordinator/executor.ts",
    "miyabi:status": "tsx src/cli/status.ts",
    "miyabi:agent": "tsx src/cli/agent.ts",
    "miyabi:auto": "tsx src/cli/auto.ts",
    "miyabi:todos": "tsx src/cli/todos.ts",
    "webhook:server": "tsx --env-file=.env src/webhook/start.ts",
    "csv:export": "tsx src/cli/export-csv.ts",
    "billing:run": "tsx src/cli/monthly-billing.ts",
    "pipeline": "tsx src/cli/pipeline.ts"
  },
  "keywords": [
    "miyabi",
    "autonomous",
    "agents",
    "shikigaku",
    "claude-code"
  ],
  "author": "PLark-droid",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@larksuiteoapi/node-sdk": "^1.59.0",
    "@line/bot-sdk": "^10.6.0",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@octokit/graphql": "^8.2.1",
    "@octokit/rest": "^21.1.1",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "csv-stringify": "^6.6.0",
    "express": "^5.2.1",
    "iconv-lite": "^0.7.2",
    "log-symbols": "^7.0.0",
    "lru-cache": "^11.0.2",
    "ora": "^8.2.0",
    "p-retry": "^6.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
